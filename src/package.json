{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run parseApi && npm run parseEnum && vite build && vite build --ssr",
    "tsc": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx, --fix --ignore-path .gitignore",
    "format": "eslint --cache --fix && prettier --write resources/js/",
    "type:check": "vue-tsc --noEmit",
    "parseEnum": "php artisan enum_to_inertia",
    "parseApi": "rimraf ./resources/js/api &&  openapi-generator-cli generate -i openapi.yaml -g typescript-axios -o ./resources/js/api --additional-properties=ngVersion=6.1.7,npmName=restClient,supportsES6=true,npmVersion=6.9.0,withInterfaces=true && cd resources/js/api && npm i && npm audit fix --force",
    "prepare": "husky"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/node": "^20.8.10",
    "@types/stripe-v3": "^3.1.32",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/server-renderer": "^3.3.7",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^9.0.11",
    "laravel-vite-plugin": "^0.8.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.8.22",
    "ziggy-js": "^1.8.1"
  },
  "dependencies": {
    "@inertiajs/vue3": "^1.0.0",
    "@stripe/stripe-js": "^2.2.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "axios": "^1.1.2",
    "ress": "^5.0.2",
    "swiper": "^11.0.5",
    "vue": "^3.2.41"
  }
}
